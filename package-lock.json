npm install
yarn install
git add package-lock.json  # Or yarn.lock
git commit -m "Add package-lock.json"
git push
- name: Cache node modules
  uses: actions/cache@v2
  with:
    path: ~/.npm
    key: ${{ runner.os }}-node-${{ hashFiles('**/package-lock.json') }}
    restore-keys: |
      ${{ runner.os }}-node-
